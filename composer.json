{
  "name":              "go1/util_index",
  "prefer-stable":     true,
  "minimum-stability": "stable",
  "autoload":          {
    "psr-4": {
      "go1\\util_index\\": "./"
    }
  },
  "require":           {
    "ext-json":                    "*",
    "ext-pdo":                     "*",
    "beberlei/assert":             "^2.5",
    "ezyang/htmlpurifier":         "^4.7.0",
    "elasticsearch/elasticsearch": "^5.0",
    "go1/app":                     "dev-master",
    "go1/util":                    "dev-master",
    "go1/util_core":               "dev-master",
    "go1/util_core_clients":       "dev-master",
    "go1-core/group/group-schema": "dev-master",
    "monolog/monolog":             "^1.19.0",
    "ongr/elasticsearch-dsl":      "^5.0"
  },
  "repositories":  [
    {
      "type": "vcs",
      "url":  "git@code.go1.com.au:go1-core/group/group-schema.git"
    }
  ],
  "scripts":           {
    "lint": "php-cs-fixer fix --using-cache=no --rules='{\"braces\": {\"allow_single_line_closure\": true}}' ."
  }
